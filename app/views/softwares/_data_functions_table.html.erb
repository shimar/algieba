<% if @data_functions.empty? %>
  <table class="table">
    <tr><td>No data functions.</td></tr>
  </table>
<% else %>
  <table class="table">
    <thead>
      <tr>
        <th>#</th>
        <th>Name</th>
        <th>Type</th>
        <th>DET</th>
        <th>RET</th>
        <th>FP</th>
      </tr>
    </thead>
    <tbody>
      <% @data_functions.each_with_index do |df, index| %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= df.name %></td>
          <td><%= df.type %></td>
          <td><%= df.det %></td>
          <td><%= df.ret %></td>
          <td><%= df.function_point %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
